{
  "name": "inugami-ng",
  "version": "0.0.4",
  "license": "GPL-3.0-only",
  "description": "Inugami project angular library",
  "keywords": ["inugami",  "angular", "components"],
  "homepage": "https://inugami.io",
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/inugamiio/inugami-ng.git"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "ng serve --port 4300"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "bootstrap": "5.3.8",
    "jquery":"3.6.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "highlight.js": "^11.11.1",
    "ngx-highlight-js": "^11.0.0",
    "ngx-google-analytics":"14.0.1",
    "inugami-icons":"0.0.8"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
